                <outMessageType>
                    <plainStringOutMessage>
                        <messageTemplate> Plan : ${plan/planID} - PlanItem : ${planItem/planItemID} - ${messageCode}:${message}</messageTemplate>
                    </plainStringOutMessage>
                </outMessageType>
            </actionNotification>
        </action>
    </ecaRule>
